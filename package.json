{
  "name": "gatsby-typescript-material-rebass-starter",
  "private": true,
  "description": "A starter to bootstrap quickly a gatsby typescript project with usual libraries and configurations",
  "version": "1.0.0",
  "author": "Abdullah Hilson <abdullah.hilson@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache public node_modules/.cache",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --watch --config codegen.yml",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.5.0",
    "@babel/runtime": "^7.5.1",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@material/react-button": "^0.14.1",
    "@material/react-card": "^0.14.1",
    "@rebass/emotion": "^3.1.1",
    "core-js": "^2.6.9",
    "emotion-theming": "^10.0.14",
    "gatsby": "^2.13.7",
    "gatsby-image": "^2.2.4",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-manifest": "^2.2.1",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-sharp": "^2.2.3",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-sharp": "^2.2.1",
    "gatsby-transformer-yaml": "^2.2.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/typescript": "1.3.1",
    "@graphql-codegen/typescript-operations": "1.3.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/rebass": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-preset-gatsby-package": "^0.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "prettier": "^1.18.2",
    "react-testing-library": "^8.0.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "emotion",
    "styled system",
    "rebass",
    "material.io",
    "eslint"
  ],
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/abumalick/gatsby-typescript-material-rebass-starter"
  },
  "bugs": {
    "url": "https://github.com/abumalick/gatsby-typescript-material-rebass-starter/issues"
  }
}
